@import './reset.styl'
@import './font.styl'
@import './mixin.styl'

// 全局样式
body
  font-family 'Alibaba Sans', 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', 'Microsoft YaHei', '微软雅黑', Arial, sans-serif
  font-size 14px
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  background-color #f1f1f1
// 公用样式
.left
  float left
.right
  float right
.hidden
  overflow hidden
.clear
  clear both

.container
    width 1200px
    margin 0 auto
